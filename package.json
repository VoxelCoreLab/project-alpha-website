    "firebase": "^12.8.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "vue3-carousel": "^0.15.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.4",
    "daisyui": "^5.5.16",
    "swagger-typescript-api": "^13.2.16",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.6.2",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
